@article{formoso_parkit_2014,
	title = {{ParkIt} - {Plataforma} inteligente de estacionamiento público},
	copyright = {Derechos de autor 2019 Agustín Formoso, Agustín Mazzilli, Rafael Sotelo},
	issn = {2301-1106},
	url = {http://revistas.um.edu.uy/index.php/ingenieria/article/view/351},
	abstract = {Resumen
					A nivel mundial las ciudades tienen un crecimiento cada vez mayor, y en muchos casos la infraestructura de tránsito que permite la movilización de su población comienza a llegar a su capacidad máxima. Distintos puntos de estas ciudades se ven afectados de una forma negativa por la gran cantidad de vehículos que intentan acceder a zonas congestionadas constantemente, y eso hace que encontrar un estacionamiento público donde aparcar el automóvil muchas veces sea dificultoso. En este marco surge el proyecto ParkIt, una plataforma que brinda información en  tiempo real acerca de la disponibilidad de estacionamiento en la vía pública. Esta plataforma permite obtener el máximo provecho de cada plaza de estacionamiento disponible, y disminuir los retrasos ocasionados por la búsqueda de las mismas. Al tener conocimiento de la ubicación de lugares disponibles, los usuarios de la plataforma se encontrarán menos tiempo en búsqueda activa de estacionamiento, lo que ayuda a descongestionar estas zonas.},
	language = {es},
	number = {12},
	urldate = {2023-05-19},
	journal = {Memoria Investigaciones en Ingeniería},
	author = {Formoso, Agustín and Mazzilli, Agustín and Sotelo, Rafael},
	month = nov,
	year = {2014},
	note = {Number: 12},
	keywords = {ciudades inteligentes, estacionamiento inteligente, Ingeniería de tránsito, Internet de las cosas},
	pages = {85--94},
	file = {Full Text PDF:/home/lautaroandressaez/Zotero/storage/W7BSQZ9M/Formoso et al. - 2014 - ParkIt - Plataforma inteligente de estacionamiento.pdf:application/pdf},
